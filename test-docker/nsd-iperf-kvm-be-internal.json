{
  "name":"iperf-NS",
  "vendor":"fokus",
  "version":"0.1",
  "vnfd":[
    {
      "vendor":"fokus",
      "version":"0.1",
      "name":"iperf-server",
      "type":"server",
      "endpoint":"generic",
      "configurations":{
        "name":"config_name",
        "configurationParameters":[]
      },
      "vdu":[
        {
          "vm_image":[
            "ubuntu-14.04-server-cloudimg-amd64-disk1"
          ],
          "vimInstanceName":"internal-kvm",
          "scale_in_out":1,
          "vnfc":[
            {
              "connection_point":[
                {
                  "floatingIp":"random",
                  "virtual_link_reference":"private"
                }
              ]
            }
          ]
        }
      ],
      "virtual_link":[
        {
          "name":"private",
          "qos":[
            "minimum_bandwith:SILVER"
          ]
        }
      ],
      "lifecycle_event":[
        {
          "event":"INSTANTIATE",
          "lifecycle_events":[
            "install.sh",
            "install-srv.sh"
          ]
        }
      ],
      "deployment_flavour":[
        {
          "flavour_key":"m1.small"
        }
      ],
      "vnfPackageLocation":"https://github.com/charliemaiors/scripts-test.git"
    },
    {
      "vendor":"fokus",
      "version":"0.1",
      "name":"iperf-client",
      "type":"client",
      "endpoint":"generic",
      "configurations":{
        "name":"config_name",
        "configurationParameters":[

        ]
      },
      "vdu":[
        {
          "vm_image":[
            "ubuntu-14.04-server-cloudimg-amd64-disk1"
          ],
          "vimInstanceName":"internal-kvm",
          "scale_in_out":1,
          "vnfc":[
            {
              "connection_point":[
                {
                  "floatingIp":"random",
                  "virtual_link_reference":"private"
                }
              ]
            }
          ]
        }
      ],
      "virtual_link":[
        {
          "name":"private"
        }
      ],
      "lifecycle_event":[
        {
          "event":"INSTANTIATE",
          "lifecycle_events":[
            "install.sh"
          ]
